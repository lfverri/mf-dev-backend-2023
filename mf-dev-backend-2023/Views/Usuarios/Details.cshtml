@model mf_dev_backend_2023.Models.Usuario

@{
    ViewData["Title"] = "Details";
}

<h1>Dados do Usuário</h1>

<div>
    <h4>Usuario</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
       
        
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Perfil)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Perfil)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" class=" btn btn-info">Voltar</a> |
    <a asp-action="Edit" class=" btn btn-warning" asp-route-id="@Model?.Id">Editar</a>
</div>
